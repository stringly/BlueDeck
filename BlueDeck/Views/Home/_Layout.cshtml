<!DOCTYPE html>

<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <link asp-href-include="lib/bootstrap/css/*.min.css" rel="stylesheet" />
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/lib/compatibility/classList.js"></script>
    <link href="~/lib/bluedeck/css/bluedeck.css" rel="stylesheet" />
    <link rel="shortcut icon" href="~/lib/bluedeck/css/images/favicon.ico" />
    <title>@ViewBag.Title</title>
</head>
<body>
    @await Component.InvokeAsync("NavigationMenu")
    <div class="col-xs-12 pageContentContainer">
        @RenderBody()
    </div>
    @await Component.InvokeAsync("FooterMenu")
</body>
</html>